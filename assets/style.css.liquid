/* reset.css */
html, body, div, span, applet, object, iframe, h1, h2, h3, h4, h5, h6, p, blockquote, pre, a, abbr, acronym, address, big, cite, code, 
del, dfn, em, font, img, ins, kbd, q, s, samp, small, strike, strong, sub, sup, tt, var, b, u, i, center, dl, dt, dd, ol, ul, li, 
fieldset, form, label, legend, table, caption, tbody, tfoot, thead, tr, th, td, input, textarea, button
{margin: 0;padding: 0;border: 0;outline: none !important;vertical-align: top;background: transparent;font-weight: normal;line-height: 1.5em;}

ol, ul {list-style:none;}
a, span {vertical-align:baseline;}
b, strong {font-weight: bold;}
i, dfn {font-style: italic;}

img {max-width: 100%;}



/* ************************************* VARIABLES ************************************* */

/* GRADIENTS */
{% capture gradient1 %}
background: {{ settings.gradient1_top }}; /* Old browsers */
background: -webkit-linear-gradient(top, {{ settings.gradient1_top }} 0%,{{ settings.gradient1_bottom }} 100%); /* Chrome10+,Safari5.1+ */
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,{{ settings.gradient1_top }}), color-stop(100%,{{ settings.gradient1_bottom }})); /* Chrome,Safari4+ */
background: -moz-linear-gradient(top, {{ settings.gradient1_top }} 0%, {{ settings.gradient1_bottom }} 100%); /* FF3.6+ */
background: -ms-linear-gradient(top, {{ settings.gradient1_top }} 0%,{{ settings.gradient1_bottom }} 100%); /* IE10+ */
background: -o-linear-gradient(top, {{ settings.gradient1_top }} 0%,{{ settings.gradient1_bottom }} 100%); /* Opera 11.10+ */
background: linear-gradient(to bottom, {{ settings.gradient1_top }} 0%,{{ settings.gradient1_bottom }} 100%); /* W3C */
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='{{ settings.gradient1_top }}', endColorstr='{{ settings.gradient1_bottom }}',GradientType=0 ); /* IE6-8 */
{% endcapture %}

{% capture gradient1_invert %}
background: {{ settings.gradient1_bottom }}; /* Old browsers */
background: -webkit-linear-gradient(top, {{ settings.gradient1_bottom }} 0%,{{ settings.gradient1_top }} 100%); /* Chrome10+,Safari5.1+ */
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,{{ settings.gradient1_bottom }}), color-stop(100%,{{ settings.gradient1_top }})); /* Chrome,Safari4+ */
background: -moz-linear-gradient(top, {{ settings.gradient1_bottom }} 0%, {{ settings.gradient1_top }} 100%); /* FF3.6+ */
background: -ms-linear-gradient(top, {{ settings.gradient1_bottom }} 0%,{{ settings.gradient1_top }} 100%); /* IE10+ */
background: -o-linear-gradient(top, {{ settings.gradient1_bottom }} 0%,{{ settings.gradient1_top }} 100%); /* Opera 11.10+ */
background: linear-gradient(to bottom, {{ settings.gradient1_bottom }} 0%,{{ settings.gradient1_top }} 100%); /* W3C */
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='{{ settings.gradient1_bottom }}', endColorstr='{{ settings.gradient1_top }}',GradientType=0 ); /* IE6-8 */
{% endcapture %}


/* CSS3 TRANSITION */
{% capture css3trans %}
-webkit-transition: 0.2s ease-in-out;
   -moz-transition: 0.2s ease-in-out;
    -ms-transition: 0.2s ease-in-out;
     -o-transition: 0.2s ease-in-out;
        transition: 0.2s ease-in-out;
{% endcapture %}



/* ************************************* GLOBAL THEME STYLES ************************************* */

/* hiding placeholder text on focus in webkit, thanks to http://www.kolodvor.net/2012/03/23/webkit-placeholder-attribute-behavior */
input:focus::-webkit-input-placeholder,
textarea:focus::-webkit-input-placeholder {color: transparent;}


/* clearfix - nicolasgallagher.com/micro-clearfix-hack/ */
.clearfix {zoom: 1;}

.clearfix:before,
.clearfix:after {content: "";display: table;}

.clearfix:after {clear: both;}


/* utility classes: hidden, float left, float right, etc. */
.hidden,
.visuallyhidden {display: none;}
 
.left {float: left;}
.right {float: right;}


/* custom selects global styles */
.customselect_wrap {position: relative;cursor: pointer;}
.customselect_wrap select {margin: 0;padding: 0;border: none;z-index: 100;cursor: pointer;
-webkit-box-sizing: border-box;
   -moz-box-sizing: border-box;
        box-sizing: border-box;
}
.customselect_wrap span.customSelect {
-webkit-box-sizing: border-box;
   -moz-box-sizing: border-box;
        box-sizing: border-box;
}


/* hover transitions */
.trs_hover {
-webkit-transition: 0.6s ease-in-out;
   -moz-transition: 0.6s ease-in-out;
    -ms-transition: 0.6s ease-in-out;
     -o-transition: 0.6s ease-in-out;
        transition: 0.6s ease-in-out;
}
.trs_hover:hover {
-webkit-transition: 0.3s ease-in-out;
   -moz-transition: 0.3s ease-in-out;
    -ms-transition: 0.3s ease-in-out;
     -o-transition: 0.3s ease-in-out;
        transition: 0.3s ease-in-out;
}



